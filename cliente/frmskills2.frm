private sub image1_click(index as integer)
on error resume next

dim k, i as integer
call playwaveds(snd_click)
    
select case index
    case 1
        if frmmain.socket1.connected then frmmain.socket1.disconnect
        
        i = 1
        for each k in text1
            userskills(i) = k.caption
            i = i + 1
        next
        me.visible = false
        frmmain.socket1.hostname = frmconnect.iptxt.text
        frmmain.socket1.remoteport = frmconnect.porttxt.text
        sendnewchar = true
        me.mousepointer = 11
        frmmain.socket1.connect
        me.mousepointer = 1
    case 0
        unload me
        frmskills.show vbmodal
end select
end sub

private sub command1_click(index as integer)
call playwaveds(snd_click)
dim indice
if index mod 2 = 0 then
    if skillpoints > 0 then
        indice = index \ 2
        text1(indice).caption = val(text1(indice).caption) + 1
        skillpoints = skillpoints - 1
    end if
else
    if skillpoints < 10 then
        
        indice = index \ 2
        if val(text1(indice).caption) > 0 then
            text1(indice).caption = val(text1(indice).caption) - 1
            skillpoints = skillpoints + 1
        end if
    end if
end if

puntos.caption = "puntos:" & skillpoints

end sub

